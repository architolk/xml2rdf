@prefix schema: <urn:file:input.xsd#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xs:    <http://www.w3.org/2001/XMLSchema> .

schema:itemtype.title
        a           xs:Element ;
        rdfs:label  "title" ;
        xs:index    "1" ;
        xs:type     schema:stringtype .

schema:orderidtype  a   xs:SimpleType ;
        rdfs:label      "orderidtype" ;
        xs:restriction  [ a           xs:Restriction ;
                          xs:base     xs:string ;
                          xs:pattern  "[0-9]{6}"
                        ] .

schema:shipordertype.orderperson
        a           xs:Element ;
        rdfs:label  "orderperson" ;
        xs:index    "1" ;
        xs:type     schema:stringtype .

schema:shiptotype.address
        a           xs:Element ;
        rdfs:label  "address" ;
        xs:index    "2" ;
        xs:type     schema:stringtype .

schema:itemtype.quantity
        a           xs:Element ;
        rdfs:label  "quantity" ;
        xs:index    "3" ;
        xs:type     schema:inttype .

schema:shiptotype.city
        a           xs:Element ;
        rdfs:label  "city" ;
        xs:index    "3" ;
        xs:type     schema:stringtype .

schema:dectype  a       xs:SimpleType ;
        rdfs:label      "dectype" ;
        xs:restriction  [ a        xs:Restriction ;
                          xs:base  xs:decimal
                        ] .

schema:itemtype  a   xs:ComplexType ;
        rdfs:label   "itemtype" ;
        xs:sequence  schema:itemtype.price , schema:itemtype.quantity , schema:itemtype.note , schema:itemtype.title .

schema:shipordertype.item
        a             xs:Element ;
        rdfs:label    "item" ;
        xs:index      "3" ;
        xs:maxOccurs  "unbounded" ;
        xs:type       schema:itemtype .

schema:shiporder  a  xs:Element ;
        rdfs:label  "shiporder" ;
        xs:type     schema:shipordertype .

schema:  a          xs:Schema ;
        rdfs:label  "urn:file:input.xsd#" ;
        xs:element  schema:shiporder .

schema:itemtype.note  a  xs:Element ;
        rdfs:label    "note" ;
        xs:index      "2" ;
        xs:minOccurs  "0" ;
        xs:type       schema:stringtype .

schema:stringtype  a    xs:SimpleType ;
        rdfs:label      "stringtype" ;
        xs:restriction  [ a        xs:Restriction ;
                          xs:base  xs:string
                        ] .

schema:shipordertype.shipto
        a           xs:Element ;
        rdfs:label  "shipto" ;
        xs:index    "2" ;
        xs:type     schema:shiptotype .

schema:inttype  a       xs:SimpleType ;
        rdfs:label      "inttype" ;
        xs:restriction  [ a        xs:Restriction ;
                          xs:base  xs:positiveInteger
                        ] .

schema:shipordertype  a  xs:ComplexType ;
        rdfs:label    "shipordertype" ;
        xs:attribute  schema:shipordertype.orderid ;
        xs:sequence   schema:shipordertype.item , schema:shipordertype.shipto , schema:shipordertype.orderperson .

schema:itemtype.price
        a           xs:Element ;
        rdfs:label  "price" ;
        xs:index    "4" ;
        xs:type     schema:dectype .

schema:shipordertype.orderid
        a           xs:Attribute ;
        rdfs:label  "orderid" ;
        xs:type     schema:orderidtype ;
        xs:use      "required" .

schema:shiptotype  a  xs:ComplexType ;
        rdfs:label   "shiptotype" ;
        xs:sequence  schema:shiptotype.country , schema:shiptotype.city , schema:shiptotype.address , schema:shiptotype.name .

schema:shiptotype.country
        a           xs:Element ;
        rdfs:label  "country" ;
        xs:index    "4" ;
        xs:type     schema:stringtype .

schema:shiptotype.name
        a           xs:Element ;
        rdfs:label  "name" ;
        xs:index    "1" ;
        xs:type     schema:stringtype .
